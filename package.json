    "bad-words": "^3.0.4",
    "dayjs": "^1.11.7",
    "dotenv": "^16.1.3",
    "dotenv-vault": "^1.22.0",
    "eslint": "^8.36.0",
    "eslint-plugin-svelte": "^2.28.0",
    "husky": "^8.0.0",
    "idb": "^7.1.1",
    "kysely": "^0.24.2",
    "kysely-d1": "^0.3.0",
