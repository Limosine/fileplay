    "tslib": "^2.4.1",
    "typescript": "^5.1.3",
    "vite": "^4.0.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.14.4",
    "webpush-webcrypto": "^1.0.3",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^6.5.4",
