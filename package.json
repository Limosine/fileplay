{
  "name": "fileplay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "postinstall": "husky install && rimraf .husky/pre-commit && echo 'Please install and login to dotenv-vault to access the required .env files'",
    "husky:pre-commit": "dotenv-vault build --yes && dotenv-vault push && dotenv-vault push production",
    "pwassets": "npx pwa-asset-generator ./static/favicon.png ./static/pwa-assets/ -v /pwa-assets -o -b '#202124' -e -p 15%"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@sveltejs/adapter-cloudflare": "^2.2.0",
    "@sveltejs/kit": "^1.5.0",
    "@types/bad-words": "^3.0.1",
    "@types/qrcode": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vite-pwa/sveltekit": "^0.2.5",
    "bad-words": "^3.0.4",
    "beercss": "^3.2.13",
    "dayjs": "^1.11.7",
    "dotenv": "^16.1.3",
    "dotenv-vault": "^1.22.0",
    "eslint": "^8.46.0",
    "eslint-plugin-svelte": "^2.28.0",
    "husky": "^8.0.0",
    "kolorist": "^1.8.0",
    "kysely": "^0.26.1",
    "kysely-d1": "^0.3.0",
    "nanoid": "^4.0.2",
    "openpgp": "^5.9.0",
    "peerjs": "^1.5.0-rc.1",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "pwa-asset-generator": "^6.3.1",
    "qrcode": "^1.5.3",
    "rimraf": "^5.0.1",
    "svelte": "^4.1.2",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.1.3",
    "vite": "^4.0.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.16.4",
    "webpush-webcrypto": "^1.0.3",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-window": "^7.0.0",
    "wrangler": "^3.0.1"
  },
  "type": "module"
}
