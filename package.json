{
  "name": "fileplay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "graphql-codegen": "graphql-codegen",
    "dev": "graphql-codegen && vite dev",
    "build": "graphql-codegen && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && node .github/workflows/validate-schema.js",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "postinstall": "husky install && rimraf .husky/pre-commit && echo 'Please install and login to dotenv-vault to access the required .env files'",
    "husky:pre-commit": "dotenv-vault build --yes && dotenv-vault push && dotenv-vault push production",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.14",
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@graphql-codegen/typed-document-node": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@smui/banner": "^7.0.0-beta.8",
    "@smui/button": "^7.0.0-beta.8",
    "@smui/card": "^7.0.0-beta.8",
    "@smui/common": "^7.0.0-beta.8",
    "@smui/dialog": "^7.0.0-beta.8",
    "@smui/drawer": "^7.0.0-beta.8",
    "@smui/fab": "^7.0.0-beta.8",
    "@smui/icon-button": "^7.0.0-beta.8",
    "@smui/linear-progress": "^7.0.0-beta.0",
    "@smui/list": "^7.0.0-beta.8",
    "@smui/paper": "^7.0.0-beta.8",
    "@smui/segmented-button": "^7.0.0-beta.8",
    "@smui/select": "^7.0.0-beta.8",
    "@smui/textfield": "^7.0.0-beta.8",
    "@smui/tooltip": "^7.0.0-beta.8",
    "@smui/top-app-bar": "^7.0.0-beta.8",
    "@sveltejs/adapter-cloudflare": "^2.2.0",
    "@sveltejs/kit": "^1.5.0",
    "@types/bad-words": "^3.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.194",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@urql/core": "^4.0.7",
    "@vite-pwa/sveltekit": "^0.1.3",
    "bad-words": "^3.0.4",
    "dgraph-js": "^21.3.1",
    "dotenv-vault": "^1.22.0",
    "dotenv-vault-core": "^0.7.0",
    "eslint": "^8.36.0",
    "eslint-plugin-svelte": "^2.28.0",
    "graphql": "^16.6.0",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "graphql-config": "^4.5.0",
    "husky": "^8.0.0",
    "idb": "^7.1.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.10.0",
    "rimraf": "^5.0.1",
    "sharp": "^0.31.3",
    "smui-theme": "^7.0.0-beta.8",
    "subscriptions-transport-ws": "^0.11.0",
    "svelte": "^3.54.0",
    "svelte-apollo": "^0.5.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.0.0",
    "vite-plugin-pwa": "^0.14.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4"
  },
  "type": "module",
  "dependencies": {
    "peerjs": "^1.4.7",
    "rtc-socket-connector-client": "^0.1.7",
    "rtc-socket-connector-server": "^0.1.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "yarn": "^1.22.19"
  }
}
