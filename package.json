{
  "name": "fileplay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "postinstall": "husky install && rimraf .husky/pre-commit && echo 'Please install and login to dotenv-vault to access the required .env files'",
    "husky:pre-commit": "dotenv-vault build --yes && dotenv-vault push && dotenv-vault push production",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "pwassets": "npx pwa-asset-generator ./static/logo.png ./static/pwa-assets/ -w -v /pwa-assets -o -b '#1a1a1a'"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@material/typography": "^14.0.0",
