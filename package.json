{
  "name": "fileplay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "postinstall": "husky install && rimraf .husky/pre-commit && echo 'Please install and login to dotenv-vault to access the required .env files'",
    "husky:pre-commit": "dotenv-vault build --yes && dotenv-vault push && dotenv-vault push production",
    "pwassets": "npx pwa-asset-generator ./static/favicon.svg ./static/pwa-assets/ -v /pwa-assets -o -b '#201a19' -e -p 15%"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^2.0.2",
    "@sveltejs/kit": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@thaunknown/simple-peer": "^9.12.1",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@types/bad-words": "^3.0.3",
    "@types/qrcode": "^1.5.5",
    "@types/simple-peer": "^9.11.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vite-pwa/sveltekit": "^0.3.0",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "bad-words": "^3.0.4",
    "beercss": "^3.4.11",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "dotenv-vault": "^1.25.0",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "husky": "^8.0.3",
    "kysely": "^0.27.0",
    "kysely-d1": "^0.3.0",
    "nanoid": "^5.0.4",
    "openpgp": "^5.11.0",
    "peerjs": "^1.5.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "pwa-asset-generator": "^6.3.1",
    "qrcode": "^1.5.3",
    "rimraf": "^5.0.5",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "trpc-sveltekit": "^3.5.22",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-window": "^7.0.0",
    "ws": "^8.16.0",
    "yarn-upgrade-all": "^0.7.2"
  },
  "type": "module",
  "packageManager": "yarn@4.0.2"
}